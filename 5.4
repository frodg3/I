class House:
    houses_history = []

    def __new__(cls, name, number_of_floors):
        instance = super(House, cls).__new__(cls)
        cls.houses_history.append(name)
        return instance

    def __init__(self, name, number_of_floors):
        self.name = name
        self.number_of_floors = number_of_floors

    def __str__(self):
        return f"House: {self.name}, Number of floors: {self.number_of_floors}"

    def demolish(self):
        print(f"The house {self.name} has been demolished, but it will remain in the history.")


# Создание трех экземпляров класса House
house1 = House("ЖК Эльбрус", 2)
print(House.houses_history)

house2 = House("ЖК Акация", 3)
print(House.houses_history)

house3 = House("ЖК Матрёшки", 1)
print(House.houses_history)

# Удаление двух экземпляров
del house1
del house2

print(House.houses_history)
